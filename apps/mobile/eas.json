{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "sim": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "image": "macos-sonoma-14.6-xcode-16.1"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "dev",
      }
    },
    "dev": {
      "developmentClient": true,
      "channel": "dev",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "dev",
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_ENV": "preview",
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "prodcution",
      "env": {
        "EXPO_PUBLIC_ENV": "prod",
      },
      "ios": {
        "image": "macos-sonoma-14.6-xcode-16.1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
